#include <stdio.h>
#include <string.h>
#include <stdlib.h>
typedef void(*func_ptr)(void);
void	n(void)
{
	const char *string = "/bin/cat /home/user/level7/.pass"
	system(string);
}
void	m(void)
{
	puts("Nope");
}
int		main(int argc, char **argv)
{
	char		*dest;
	char		*src = argv[1];
	size_t		size = 64;
	func_ptr	*func;
	dest = malloc(size);
	size = 4
	func = malloc(size);
	*func = (void *)m;
	strcpy(dest, src);
	(**func)();
	return (0);
}
